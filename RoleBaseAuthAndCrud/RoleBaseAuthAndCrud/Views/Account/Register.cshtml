@model RoleBaseAuthAndCrud.ViewModels.RegisterViewModel;

<h1>Register</h1>

<form method="post">
	<div>
		<input asp-for="UserName" placeholder="Enter your username"  />
		<span asp-validation-for="UserName"></span>
	</div>

	<div>
		<input asp-for="Email" placeholder="Enter your email" />
		<span asp-validation-for="Email"></span>
	</div>

	<div>
		<input asp-for="Password" placeholder="Enter your password"  />
		<span asp-validation-for="Password"></span>
	</div>

	<div>
		<input asp-for="Address" placeholder="Enter your address"  />
		<span asp-validation-for="Address"></span>
	</div>

	<div>
		<input asp-for="Gender" placeholder="Enter your gender" />
		<span asp-validation-for="Gender"></span>
	</div>

	<div>
		<input type="Submit" value="Register" />
	</div>

</form>

