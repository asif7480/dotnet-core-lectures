@model Session19.ViewModels.RegisterViewModel


<form method="post">
	<div>
		<input asp-for="UserName" placeholder="Enter your username" />
		<span asp-validation-for="UserName"></span>
	</div>

	<div>
		<input asp-for="Email" placeholder="Enter your email" />
		<span asp-validation-for="Email"></span>
	</div>

	<div>
		<input asp-for="Password" placeholder="Enter your password" />
		<span asp-validation-for="Password"></span>
	</div>

	<div>
		<input asp-for="Address" placeholder="Enter your address" />
		<span asp-validation-for="Address"></span>
	</div>

	<div>
		<input asp-for="City" placeholder="Enter your city" />
		<span asp-validation-for="City"></span>
	</div>

	<div>
		<input type="submit" value="Register" />
	</div>
</form>

